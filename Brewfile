
# tap repositories and their packages

## beeftornado/rmtree
tap beeftornado/rmtree
install brew-rmtree

## caskroom/cask
tap caskroom/cask
install brew-cask
cask install growlnotify

## caskroom/versions
tap caskroom/versions

## homebrew/dupes
tap homebrew/dupes
install grep
install screen
install zlib

## homebrew/php
tap homebrew/php
install composer

## homebrew/python
tap homebrew/python

## homebrew/versions
tap homebrew/versions

## mopidy/mopidy
tap mopidy/mopidy
install mopidy-spotify
install pykka
install python-backports-ssl-match-hostname
install python-certifi
install python-tornado

## mpv-player/mpv
tap mpv-player/mpv
install harfbuzz
install mpv --with-official-libass

## rcmdnk/file
tap rcmdnk/file
install brew-file

## sampsyo/py
tap sampsyo/py

## twayz/toggle-osx-shadows
tap twayz/toggle-osx-shadows
install toggle-osx-shadows

